{
  "name": "cerises-ui",
  "version": "0.0.1",
  "description": "A modern solidjs based UI library ~ it is very lightweight!",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "render-docs-routes": "node ./scripts/render-docs-routes/index.js"
  },
  "keywords": [
    "cerises-ui",
    "component library",
    "ui framework",
    "ui",
    "solidjs"
  ],
  "main": "lib/index.js",
  "homepage": "https://github.com/cerises-ui/cerises-ui",
  "repository": {
    "type": "git",
    "url": "git@github.com:cerises-ui/cerises-ui.git"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.tsx": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --parser=vue --write",
      "eslint --fix"
    ],
    "*.css": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write --parser markdown --prose-wrap never",
      "eslint --fix"
    ]
  },
  "files": [
    "es",
    "lib",
    "web-types.json",
    "README.md"
  ],
  "author": "Yorksh1re",
  "license": "MIT",
  "devDependencies": {
    "@mdx-js/rollup": "^2.3.0",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@types/fs-extra": "^11.0.1",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "commander": "^11.0.0",
    "cssnano": "^6.0.1",
    "eslint": "8.45.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fs-extra": "^11.1.1",
    "jsdom": "^22.1.0",
    "remark-gfm": "^3.0.1",
    "sass": "^1.64.1",
    "solid-app-router": "^0.4.2",
    "solid-jsx": "^0.9.1",
    "solid-testing-library": "^0.5.1",
    "solidown": "^1.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vite-plugin-solid": "^2.7.0",
    "vite-plugin-windicss": "^1.9.0",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "seemly": "^0.3.6",
    "solid-js": "^1.7.8"
  }
}
